<section class="parties-list container-fluid">
	<div class="row">
		{{input type="search" class="form-control" placeholder="חפש חבר כנסת או מפלגה" value=searchTerm}}
	</div>
	<div class="row">
		{{#each}}
			<div class="party col-xs-6 col-md-3 col-lg-2">
				<img {{bind-attr src=imageUrl}}>
				<span class="grade">{{grade}}</span>
			</div>
		{{/each}}
	</div>
</section>

{{outlet}}
